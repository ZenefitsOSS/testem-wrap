{
  "cwd": "dist",
  "launch_in_ci": [
    "Chrome"
  ],
  "launch_in_dev": [
    "PhantomJS",
    "Chrome"
  ],
  "framework": "custom",
  "routes": {
    "/vendor": "../vendor"
  },
  "browser_args": {
    "Chrome": ["--headless", "--disable-gpu", "--remote-debugging-port=9222"]
  },
  "proxies": {
    "/api": {
      "target": "http://localhost:9001"
    },
    "/custom_api": {
      "target": "http://localhost:9001"
    },
    "/internal-services": {
      "target": "http://localhost:9001"
    }
  }
}


